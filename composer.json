{
    "name": "torstenzander/blog-bundle",
    "type": "symfony-bundle",
    "description": "Simple Blog Bundle",
    "keywords": ["blog", "bundle"],
    "homepage": "http://github.com/torstenzander/tzBlogBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Torsten Zander",
            "email": "info@tzander.de"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev",
        "symfony/finder": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4",
        "doctrine/doctrine-bundle": "1.0.*",
        "friendsofsymfony/user-bundle": "1.3.2",
        "knplabs/knp-paginator-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Tz\\BlogBundleBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Tz/BlogBundle"
}